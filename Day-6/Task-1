//Array Destructuring
const vehicles = ["car", "bike", "bus", "train"];
const [v1, v2, , v4] = vehicles;
console.log(v1, v2, v4); 

//Object Destructuring (with nested)
const smartphone = {
  brand: "Samsung",
  model: "Galaxy S23",
  features: {
    camera: "108MP",
    battery: "5000mAh"
  }
};

const { model, features: { camera } } = smartphone;
console.log(model, camera); 

//Accessing object properties
console.log(smartphone.brand);      
console.log(smartphone["features"]);
